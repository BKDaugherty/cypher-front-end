{
"presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  },
  "plugins": [
    ["module-resolver", {
      "alias": {
       "@Components": "./Components",
          "@Containers": "./Containers",
          "@Actions":"./Actions",
          "@Reducers":"./Reducers",
          "@Store":"./Store",
          "@Navigation":"./Navigation",
          "@Data":"./Data",
          "@Style":"./Style",
          "@Images":"./Assets/Images",
          "@Assets":"./Assets"
      }
    }]
  ]
}